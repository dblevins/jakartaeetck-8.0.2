<sun-ejb-jar>
  <enterprise-beans>
    <unique-id>0</unique-id>
    <ejb>
      <ejb-name>ByeClientEjb</ejb-name>
      <jndi-name>ByeClientEjb</jndi-name>
      <service-ref>
        <service-ref-name>service/shared_service_ref</service-ref-name>
        <port-info>
          <service-endpoint-interface>com.sun.ts.tests.webservices.deploy.CompScopedRefs.Bye</service-endpoint-interface>
          <wsdl-port>
            <namespaceURI>http://Bye.org</namespaceURI>
            <localpart>ByePort</localpart>
          </wsdl-port>
        </port-info>
        <wsdl-override>http://webServerHost.1:webServerPort.1/CompScopeTest_web/ws4ee_2?WSDL</wsdl-override>
      </service-ref>
      <pass-by-reference>false</pass-by-reference>
      <is-read-only-bean>false</is-read-only-bean>
      <refresh-period-in-seconds>-1</refresh-period-in-seconds>
      <gen-classes/>
    </ejb>
    <ejb>
      <ejb-name>HelloClientEjb</ejb-name>
      <jndi-name>HelloClientEjb</jndi-name>
      <service-ref>
        <service-ref-name>service/shared_service_ref</service-ref-name>
        <port-info>
          <service-endpoint-interface>com.sun.ts.tests.webservices.deploy.CompScopedRefs.Hello</service-endpoint-interface>
          <wsdl-port>
            <namespaceURI>http://Hello.org</namespaceURI>
            <localpart>HelloPort</localpart>
          </wsdl-port>
        </port-info>
        <wsdl-override>http://webServerHost.1:webServerPort.1/CompScopeTest_web/ws4ee_1?WSDL</wsdl-override>
      </service-ref>
      <pass-by-reference>false</pass-by-reference>
      <is-read-only-bean>false</is-read-only-bean>
      <refresh-period-in-seconds>-1</refresh-period-in-seconds>
      <gen-classes/>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
