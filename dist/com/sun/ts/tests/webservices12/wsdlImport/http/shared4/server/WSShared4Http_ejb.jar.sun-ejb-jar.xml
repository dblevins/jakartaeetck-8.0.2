<sun-ejb-jar>
  <enterprise-beans>
    <unique-id>0</unique-id>
    <ejb>
      <ejb-name>StatelessTestBean1</ejb-name>
      <jndi-name>StatelessTestBean1</jndi-name>
      <pass-by-reference>false</pass-by-reference>
      <is-read-only-bean>false</is-read-only-bean>
      <refresh-period-in-seconds>-1</refresh-period-in-seconds>
      <gen-classes/>
      <webservice-endpoint>
        <port-component-name>TestsBean1</port-component-name>
        <endpoint-address-uri>WSShared4Http/jaxws/Tests1</endpoint-address-uri>
      </webservice-endpoint>
    </ejb>
    <ejb>
      <ejb-name>StatelessTestBean2</ejb-name>
      <jndi-name>StatelessTestBean2</jndi-name>
      <pass-by-reference>false</pass-by-reference>
      <is-read-only-bean>false</is-read-only-bean>
      <refresh-period-in-seconds>-1</refresh-period-in-seconds>
      <gen-classes/>
      <webservice-endpoint>
        <port-component-name>TestsBean2</port-component-name>
        <endpoint-address-uri>WSShared4Http/jaxws/Tests2</endpoint-address-uri>
      </webservice-endpoint>
    </ejb>
    <webservice-description>
      <webservice-description-name>Shared4HttpSvc1</webservice-description-name>
      <wsdl-publish-location>http://webServerHost.1:webServerPort.1/WSShared4Http/jaxws/Tests1?WSDL</wsdl-publish-location>

    </webservice-description>
    <webservice-description>
      <webservice-description-name>Shared4HttpSvc2</webservice-description-name>
      <wsdl-publish-location>http://webServerHost.1:webServerPort.1/WSShared4Http/jaxws/Tests2?WSDL</wsdl-publish-location>
    </webservice-description>
  </enterprise-beans>
</sun-ejb-jar>

