<AuditableEventQuery xmlns="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0"
  xmlns:lcm="urn:oasis:names:tc:ebxml-regrep:xsd:lcm:3.0" 
  xmlns:query="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0" 
  xmlns:rim="urn:oasis:names:tc:ebxml-regrep:xsd:rim:3.0" 
  xmlns:rs="urn:oasis:names:tc:ebxml-regrep:xsd:rs:3.0" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0  ../../../../../ebxmlrr-spec/misc/3.0/schema/query.xsd">
  
  <EventTypeQuery xsi:type="query:ClassificationNodeQueryType">
    <PrimaryFilter comparator="Like" domainAttribute="path"
      value="%Creat%" xsi:type="query:StringFilterType"/>
  </EventTypeQuery>

  <UserQuery xsi:type="query:UserQueryType">
    <PrimaryFilter comparator="Like" domainAttribute="id"
      value="%ikola%" xsi:type="query:StringFilterType"/>
  </UserQuery>

</AuditableEventQuery>

