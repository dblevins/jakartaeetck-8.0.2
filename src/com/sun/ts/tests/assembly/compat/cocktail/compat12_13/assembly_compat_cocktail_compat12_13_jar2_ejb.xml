  <display-name>Ejb2</display-name>
  <enterprise-beans>
    <session>
      <display-name>ReferencedBean2</display-name>
      <ejb-name>ReferencedBean2</ejb-name>
      <home>com.sun.ts.tests.assembly.compat.cocktail.compat12_13.ReferencedBeanHome</home>
