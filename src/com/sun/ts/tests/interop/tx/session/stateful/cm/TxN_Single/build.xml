<project name="interop_tx_session_stateful_cm_TxN_Single" basedir="." default="usage">
<import file="../../../../../../../../../../../bin/xml/ts.import.xml"/> 


  <target name="build.common.apps">
    <ant dir="../../../../txbean" target="build" inheritAll="false"/>
  </target>


  <target name="package" depends="compile, build.common.apps">

      <ts.ejbjar descriptor="interop_txSsfCm_TxNSingle_ejb.xml"
                 archivename="interop_txSsfCm_TxNSingle">
        <fileset dir="${class.dir}"
                 includes="com/sun/ts/tests/ejb/ee/tx/txbean/AppException.class,
                           com/sun/ts/tests/ejb/ee/tx/txbean/SysException.class,
                           com/sun/ts/tests/ejb/ee/tx/txbean/TxBean.class,
                           com/sun/ts/tests/ejb/ee/tx/txbean/TxBeanHome.class"/>
      </ts.ejbjar>

      <ts.clientjar descriptor="interop_txSsfCm_TxNSingle_client.xml"
                    archivename="interop_txSsfCm_TxNSingle"
                    mainclass="com.sun.ts.tests.interop.tx.session.stateful.cm.TxN_Single.Client"
                    excludedfiles="com/sun/ts/tests/interop/tx/session/stateful/cm/TxN_Single/TestBeanEJB.class"/>

      <ts.ear archivename="interop_txSsfCm_TxNSingle"/>

  </target>

</project>
