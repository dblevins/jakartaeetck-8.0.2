<project name="interop_csiv2_ew_ssl_ssln_upn_anonid_forward" basedir="." default="usage">
<import file="../../../../../../../../../bin/xml/ts.import.xml"/> 


  <target name="build.common.apps">
    <ant dir="../../rionly" target="build"/>
  </target>


  <target name="compile">
      <ts.javac includes="${pkg.dir}/**/*.java,
                          com/sun/ts/tests/interop/csiv2/common/**/*.java"/>
  </target>


  <target name="package" depends="compile, build.common.apps">

      <ts.ejbjar descriptor="interop_csiv2_ew_ssl_ssln_upn_anonid_f_ejb.xml"
                 archivename="interop_csiv2_ew_ssl_ssln_upn_anonid_f">
        <fileset dir="${class.dir}"
                 includes="com/sun/ts/tests/interop/csiv2/common/*.class"
                 excludes="com/sun/ts/tests/interop/csiv2/common/CSIv2AppClient.class,
                           com/sun/ts/tests/interop/csiv2/common/CSIv2Servlet.class,
                           com/sun/ts/tests/interop/csiv2/common/CSIv2Util.class,
                           com/sun/ts/tests/interop/csiv2/common/LoggingSec*.class"/>
      </ts.ejbjar>

      <ts.war descriptor="interop_csiv2_ew_ssl_ssln_upn_anonid_f_web.xml" 
              archivename="interop_csiv2_ew_ssl_ssln_upn_anonid_f">
        <zipfileset dir="${class.dir}"
                    includes="com/sun/ts/tests/interop/csiv2/common/*.class"
                    excludes="com/sun/ts/tests/interop/csiv2/common/CSIv2AppClient.class,
                              com/sun/ts/tests/interop/csiv2/common/CSIv2Util.class,
                              com/sun/ts/tests/interop/csiv2/common/CSIv2SessionBean.class
                              com/sun/ts/tests/interop/csiv2/common/LoggingSec*.class"
                    prefix="WEB-INF/classes"/>
        <fileset dir="contentRoot"/>
      </ts.war>

      <ts.clientjar descriptor="interop_csiv2_ew_ssl_ssln_upn_anonid_f_client.xml"
                    archivename="interop_csiv2_ew_ssl_ssln_upn_anonid_f"
                    mainclass="com.sun.ts.tests.interop.csiv2.ew_ssl_ssln_upn_anonid.forward.Client">
        <fileset dir="${class.dir}"
                 includes="com/sun/ts/tests/interop/csiv2/common/**/*.class"
                 excludes="com/sun/ts/tests/interop/csiv2/common/CSIv2Servlet.class,
                           com/sun/ts/tests/interop/csiv2/common/CSIv2SessionBean.class,
                           com/sun/ts/tests/interop/csiv2/common/CSIv2Util.class,
                           com/sun/ts/tests/interop/csiv2/common/LoggingSec*.class"/>
      </ts.clientjar>

      <ts.ear archivename="interop_csiv2_ew_ssl_ssln_upn_anonid_f" includedefaultfiles="false">
        <fileset dir="${dist.dir}/${pkg.dir}"
                 includes="interop_csiv2_ew_ssl_ssln_upn_anonid_f_ejb.jar,
                           interop_csiv2_ew_ssl_ssln_upn_anonid_f_web.war,
                           interop_csiv2_ew_ssl_ssln_upn_anonid_f_client.jar"/>
      </ts.ear>

      <ts.ejbjar descriptor="interop_csiv2_ew_ssl_ssln_upn_anonid_f_j2ee2_ejb.xml"
                 archivename="interop_csiv2_ew_ssl_ssln_upn_anonid_f_j2ee2">
        <fileset dir="${class.dir}"
                 includes="com/sun/ts/tests/interop/csiv2/common/*.class"
                 excludes="com/sun/ts/tests/interop/csiv2/common/CSIv2AppClient.class,
                           com/sun/ts/tests/interop/csiv2/common/CSIv2Servlet.class,
                           com/sun/ts/tests/interop/csiv2/common/CSIv2Util.class,
                           com/sun/ts/tests/interop/csiv2/common/LoggingSec*.class"/>
      </ts.ejbjar>

      <ts.ear archivename="interop_csiv2_ew_ssl_ssln_upn_anonid_f_j2ee2" includedefaultfiles="false">
        <fileset dir="${dist.dir}/${pkg.dir}" includes="interop_csiv2_ew_ssl_ssln_upn_anonid_f_j2ee2_ejb.jar"/>
      </ts.ear>

  </target>

</project>
