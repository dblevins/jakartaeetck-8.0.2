<project name="jaxrs_rs_core_linkbuilder" basedir="." default="usage">

	<property name="app.name" value="jaxrs_rs_core_linkbuilder" />

	<import file="../../../../../../../../../../bin/xml/ts.import.xml" />

	<target name="package">
		<ts.vehicles name="jaxrs_rs_core_linkbuilder" buildleveloverride="2">
			<servlet-elements>
				<zipfileset dir="${class.dir}" includes="${pkg.dir}/*.class, 
                            com/sun/ts/tests/jaxrs/common/JAXRSCommonClient*.class,
							com/sun/ts/tests/jaxrs/common/impl/TRACE.class,
				            com/sun/ts/tests/jaxrs/common/util/JaxrsUtil.class,
                            com/sun/ts/tests/common/webclient/TestFailureException.class" prefix="WEB-INF/classes" />
			</servlet-elements>
		</ts.vehicles>
	</target>

</project>
