<webservices xmlns="http://java.sun.com/xml/ns/j2ee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:wsdl="http://messagecontextservice.org/wsdl"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://www.ibm.com/webservices/xsd/j2ee_web_services_1_1.xsd"
  version="1.1">

  <webservice-description>
    <webservice-description-name>MCEjbTest</webservice-description-name>
    <wsdl-file>META-INF/wsdl/MessageContext.wsdl</wsdl-file>
    <jaxrpc-mapping-file>MessageContext.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>HelloPort</port-component-name>
      <wsdl-port>wsdl:HelloPort</wsdl-port>
      <service-endpoint-interface>com.sun.ts.tests.webservices.handlerEjb.MessageContext.Hello</service-endpoint-interface>
      <service-impl-bean>
        <ejb-link>MCEjb</ejb-link>
      </service-impl-bean>
      <handler>
         <handler-name>ServerHandler1</handler-name>
         <handler-class>com.sun.ts.tests.webservices.handlerEjb.MessageContext.ServerHandler1</handler-class>
      </handler>
      <handler>
         <handler-name>ServerHandler2</handler-name>
         <handler-class>com.sun.ts.tests.webservices.handlerEjb.MessageContext.ServerHandler2</handler-class>
      </handler>
      <handler>
         <handler-name>ServerHandler3</handler-name>
         <handler-class>com.sun.ts.tests.webservices.handlerEjb.MessageContext.ServerHandler3</handler-class>
      </handler>
    </port-component>
  </webservice-description>
</webservices>
