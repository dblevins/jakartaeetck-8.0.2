<webservices xmlns="http://java.sun.com/xml/ns/j2ee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:wsdl="http://handlerservice1.org/wsdl"
             xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://www.ibm.com/webservices/xsd/j2ee_web_services_1_1.xsd"
             version="1.1">

  <webservice-description>
    <webservice-description-name>HLCTest</webservice-description-name>
    <wsdl-file>META-INF/wsdl/HandlerService1.wsdl</wsdl-file>
    <jaxrpc-mapping-file>HandlerService1.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>HandlerLifecyclePort</port-component-name>
      <wsdl-port>wsdl:HelloPort</wsdl-port>
      <service-endpoint-interface>com.sun.ts.tests.webservices.handlerEjb.HandlerLifecycle.Hello</service-endpoint-interface>
      <service-impl-bean>
        <ejb-link>HLCEjb</ejb-link>
      </service-impl-bean>
      <handler>
	<handler-name>SHandler</handler-name>
	<handler-class>com.sun.ts.tests.webservices.handlerEjb.HandlerLifecycle.ServerHandler</handler-class>
      </handler>
    </port-component>
  </webservice-description>
</webservices>
