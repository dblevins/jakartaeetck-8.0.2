<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

  <display-name>HFlowTApp</display-name>
  <servlet>
    <servlet-name>XmlServletName</servlet-name>
    <servlet-class>com.sun.ts.tests.webservices.handler.HandlerFlow.HelloImpl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>XmlServlet2Name</servlet-name>
    <servlet-class>com.sun.ts.tests.webservices.handler.HandlerFlow.Hello2Impl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>XmlServletName</servlet-name>
    <url-pattern>/ws4ee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>XmlServlet2Name</servlet-name>
    <url-pattern>/ws4ee-2</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>54</session-timeout>
  </session-config>
  <env-entry>
	<env-entry-name>jsr109/entry1</env-entry-name>
	<env-entry-type>java.lang.String</env-entry-type>
	<env-entry-value>shadow</env-entry-value>
  </env-entry>
  <env-entry>
	<env-entry-name>jsr109/entry2</env-entry-name>
	<env-entry-type>java.lang.Integer</env-entry-type>
	<env-entry-value>8</env-entry-value>
  </env-entry>
</web-app>

