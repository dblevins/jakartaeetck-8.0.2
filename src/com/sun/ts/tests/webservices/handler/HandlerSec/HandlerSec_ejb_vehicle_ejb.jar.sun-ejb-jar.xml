<sun-ejb-jar>
  <enterprise-beans>
    <unique-id>0</unique-id>
    <ejb>
      <ejb-name>com_sun_ts_tests_common_vehicle_ejb_EJBVehicle</ejb-name>
      <jndi-name>HandlerSec_ejb_vehicle</jndi-name>
      <service-ref>
        <service-ref-name>service/handlersec</service-ref-name>
        <port-info>
          <service-endpoint-interface>com.sun.ts.tests.webservices.handler.HandlerSec.TestQuery</service-endpoint-interface>
          <wsdl-port>
            <namespaceURI>http://handlersec.org/wsdl</namespaceURI>
            <localpart>TestQueryPort</localpart>
          </wsdl-port>
        </port-info>
        <port-info>
          <service-endpoint-interface>com.sun.ts.tests.webservices.handler.HandlerSec.TestAuth</service-endpoint-interface>
          <wsdl-port>
            <namespaceURI>http://handlersec.org/wsdl</namespaceURI>
            <localpart>TestAuthPort</localpart>
          </wsdl-port>
          <stub-property>
            <name>javax.xml.rpc.security.auth.password</name>
            <value>javajoe</value>
          </stub-property>
          <stub-property>
            <name>javax.xml.rpc.security.auth.username</name>
            <value>javajoe</value>
          </stub-property>
        </port-info>
        <port-info>
          <service-endpoint-interface>com.sun.ts.tests.webservices.handler.HandlerSec.TestUnAuth</service-endpoint-interface>
          <wsdl-port>
            <namespaceURI>http://handlersec.org/wsdl</namespaceURI>
            <localpart>TestUnAuthPort</localpart>
          </wsdl-port>
          <stub-property>
            <name>javax.xml.rpc.security.auth.password</name>
            <value>j2ee</value>
          </stub-property>
          <stub-property>
            <name>javax.xml.rpc.security.auth.username</name>
            <value>j2ee</value>
          </stub-property>
        </port-info>
        <port-info>
          <service-endpoint-interface>com.sun.ts.tests.webservices.handler.HandlerSec.TestNoSec</service-endpoint-interface>
          <wsdl-port>
            <namespaceURI>http://handlersec.org/wsdl</namespaceURI>
            <localpart>TestNoSecPort</localpart>
          </wsdl-port>
        </port-info>
        <wsdl-override>http://webServerHost.1:webServerPort.1/HandlerSec_web/ws4ee?WSDL</wsdl-override>
      </service-ref>
      <pass-by-reference>false</pass-by-reference>
      <ior-security-config>
        <transport-config>
          <integrity>supported</integrity>
          <confidentiality>supported</confidentiality>
          <establish-trust-in-target>supported</establish-trust-in-target>
          <establish-trust-in-client>supported</establish-trust-in-client>
        </transport-config>
        <as-context>
          <auth-method>username_password</auth-method>
          <realm>default</realm>
          <required>false</required>
        </as-context>
        <sas-context>
          <caller-propagation>supported</caller-propagation>
        </sas-context>
      </ior-security-config>
      <is-read-only-bean>false</is-read-only-bean>
      <refresh-period-in-seconds>-1</refresh-period-in-seconds>
      <gen-classes/>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
