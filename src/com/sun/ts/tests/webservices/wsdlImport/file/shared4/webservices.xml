<webservices xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:wsdl1="http://SharedTest1.org/wsdl"
    xmlns:wsdl2="http://SharedTest2.org/wsdl"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
         http://www.ibm.com/webservices/xsd/j2ee_web_services_1_1.xsd"
      version="1.1">

  <webservice-description>
    <webservice-description-name>FileShared4Svc1</webservice-description-name>
    <wsdl-file>META-INF/wsdl/std/svc1/Svc1.wsdl</wsdl-file>
    <jaxrpc-mapping-file>Svc1.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>SharedTest1Port</port-component-name>
      <wsdl-port>wsdl1:SharedTestPort1</wsdl-port>
      <service-endpoint-interface>com.sun.ts.tests.webservices.wsdlImport.file.shared4.SharedTest1</service-endpoint-interface>
      <service-impl-bean>
        <ejb-link>FileShared4Bean1</ejb-link>
      </service-impl-bean>
    </port-component>
  </webservice-description>
  <webservice-description>
    <webservice-description-name>FileShared4Svc2</webservice-description-name>
    <wsdl-file>META-INF/wsdl/std/svc2/Svc2.wsdl</wsdl-file>
    <jaxrpc-mapping-file>Svc2.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>SharedTest2Port</port-component-name>
      <wsdl-port>wsdl2:SharedTestPort2</wsdl-port>
      <service-endpoint-interface>com.sun.ts.tests.webservices.wsdlImport.file.shared4.SharedTest2</service-endpoint-interface>
      <service-impl-bean>
        <ejb-link>FileShared4Bean2</ejb-link>
      </service-impl-bean>
    </port-component>
  </webservice-description>
</webservices>
