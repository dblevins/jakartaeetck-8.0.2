<webservices xmlns="http://java.sun.com/xml/ns/j2ee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:wsdl1="http://Hello.org"
             xmlns:wsdl2="http://Bye.org"
             xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://www.ibm.com/webservices/xsd/j2ee_web_services_1_1.xsd"
             version="1.1">

  <webservice-description>
    <webservice-description-name>FileTwin2TestHello</webservice-description-name>
    <wsdl-file>WEB-INF/wsdl/svc1/HelloService.wsdl</wsdl-file>
    <jaxrpc-mapping-file>HelloService.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>HelloPC</port-component-name>
      <wsdl-port>wsdl1:HelloPort</wsdl-port>
      <service-endpoint-interface>com.sun.ts.tests.webservices.wsdlImport.file.twin2.Hello</service-endpoint-interface>
      <service-impl-bean>
        <servlet-link>XmlServletName1</servlet-link>
      </service-impl-bean>
    </port-component>
  </webservice-description> 

  <webservice-description>
    <webservice-description-name>FileTwin2TestBye</webservice-description-name>
    <wsdl-file>WEB-INF/wsdl/svc2/ByeService.wsdl</wsdl-file>
    <jaxrpc-mapping-file>ByeService.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>ByePC</port-component-name>
      <wsdl-port>wsdl2:ByePort</wsdl-port>
      <service-endpoint-interface>com.sun.ts.tests.webservices.wsdlImport.file.twin2.Bye</service-endpoint-interface>
      <service-impl-bean>
        <servlet-link>XmlServletName2</servlet-link>
      </service-impl-bean>
    </port-component>
  </webservice-description> 
</webservices>
