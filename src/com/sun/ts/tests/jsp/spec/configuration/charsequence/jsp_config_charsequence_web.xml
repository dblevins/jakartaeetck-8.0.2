<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>JspConfigCharsequence</display-name>
  <session-config>
    <session-timeout>5</session-timeout>
  </session-config>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>/deferredSyntaxAllowedAsLiteralTrueTemplateTextTest.jsp</url-pattern>
      <deferred-syntax-allowed-as-literal>true</deferred-syntax-allowed-as-literal>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/deferredSyntaxAllowedAsLiteralFalseTemplateTextTest.jsp</url-pattern>
      <deferred-syntax-allowed-as-literal>false</deferred-syntax-allowed-as-literal>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/deferredSyntaxAllowedAsLiteralTrueActionTest.jsp</url-pattern>
      <deferred-syntax-allowed-as-literal>true</deferred-syntax-allowed-as-literal>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/deferredSyntaxAllowedAsLiteralFalseActionTest.jsp</url-pattern>
      <deferred-syntax-allowed-as-literal>false</deferred-syntax-allowed-as-literal>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/noDeferredSyntaxAllowedAsLiteralTemplateTextTest.jsp</url-pattern>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/noDeferredSyntaxAllowedAsLiteralActionTest.jsp</url-pattern>
    </jsp-property-group>
  </jsp-config>
</web-app>
