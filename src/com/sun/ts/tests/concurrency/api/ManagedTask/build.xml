<project name="concurrency_api_managedTask" basedir="." default="usage">

    <import file="../../../../../../../../bin/xml/ts.import.xml"/>

    <property name="app.name" value="${ant.project.name}"/>

    <property name="common" value="com/sun/ts/tests/concurrency/api/common"/>

    <target name="compile">
        <ts.javac includes="${pkg.dir}/**/*.java,
                        ${common}/**/*.java"/>
    </target>

    <target name="package">
        <ts.vehicles name="${app.name}" singleear="true" classes="${common}/**/*.class">
        </ts.vehicles>
    </target>
</project>
