<webservices xmlns="http://java.sun.com/xml/ns/j2ee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:wsdl="http://marshalltestservice.org/MarshallTestService.wsdl"
             xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://www.ibm.com/webservices/xsd/j2ee_web_services_1_1.xsd"
             version="1.1">

    <webservice-description>
         <webservice-description-name>WSIRLMarshallTest</webservice-description-name>
         <wsdl-file>WEB-INF/wsdl/MarshallTestService.wsdl</wsdl-file>
         <jaxrpc-mapping-file>MarshallTestService.xml</jaxrpc-mapping-file>
         <port-component>
             <port-component-name>MarshallTestPort1</port-component-name>
             <wsdl-port>wsdl:MarshallTestPort1</wsdl-port>
             <service-endpoint-interface>com.sun.ts.tests.jaxrpc.ee.wsi.rpc.literal.marshalltest.MarshallTest</service-endpoint-interface>
             <service-impl-bean>
                 <servlet-link>XmlServletName1</servlet-link>
             </service-impl-bean>
         </port-component>
         <port-component>
             <port-component-name>MarshallTestPort2</port-component-name>
             <wsdl-port>wsdl:MarshallTestPort2</wsdl-port>
             <service-endpoint-interface>com.sun.ts.tests.jaxrpc.ee.wsi.rpc.literal.marshalltest.NewSchemaTest</service-endpoint-interface>
             <service-impl-bean>
                 <servlet-link>XmlServletName2</servlet-link>
             </service-impl-bean>
         </port-component>
         <port-component>
             <port-component-name>MarshallTestPort3</port-component-name>
             <wsdl-port>wsdl:MarshallTestPort3</wsdl-port>
             <service-endpoint-interface>com.sun.ts.tests.jaxrpc.ee.wsi.rpc.literal.marshalltest.CompoundTest</service-endpoint-interface>
             <service-impl-bean>
                 <servlet-link>XmlServletName3</servlet-link>
             </service-impl-bean>
         </port-component>
         <port-component>
             <port-component-name>MarshallTestPort4</port-component-name>
             <wsdl-port>wsdl:MarshallTestPort4</wsdl-port>
             <service-endpoint-interface>com.sun.ts.tests.jaxrpc.ee.wsi.rpc.literal.marshalltest.OneWayTest</service-endpoint-interface>
             <service-impl-bean>
                 <servlet-link>XmlServletName4</servlet-link>
             </service-impl-bean>
         </port-component>
    </webservice-description> 
</webservices>
