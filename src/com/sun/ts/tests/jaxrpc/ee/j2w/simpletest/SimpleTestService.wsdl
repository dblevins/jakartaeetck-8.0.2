<definitions name="SimpleTestService" targetNamespace="http://simpletestservice.org/wsdl" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://simpletestservice.org/wsdl" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns="http://schemas.xmlsoap.org/wsdl/">
  <types>
  </types>
  <message name="Hello_helloResponse">
    <part name="result" type="xsd:string"/>
  </message>
  <message name="Hello_hello">
    <part name="String_1" type="xsd:string"/>
  </message>
  <message name="Hello_helloOneWay">
    <part name="String_1" type="xsd:string"/>
  </message>
  <portType name="Hello">
    <operation name="hello" parameterOrder="String_1">
      <input message="tns:Hello_hello"/>
      <output message="tns:Hello_helloResponse"/>
    </operation>
    <operation name="helloOneWay">
      <input message="tns:Hello_helloOneWay"/>
    </operation>
  </portType>
  <binding name="HelloBinding" type="tns:Hello">
    <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
    <operation name="hello">
      <soap:operation soapAction=""/>
      <input>
        <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://simpletestservice.org/wsdl"/>
      </input>
      <output>
        <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://simpletestservice.org/wsdl"/>
      </output>
    </operation>
    <operation name="helloOneWay">
      <soap:operation soapAction=""/>
      <input>
        <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://simpletestservice.org/wsdl"/>
      </input>
    </operation>
  </binding>
  <service name="SimpleTestService">
    <port name="HelloPort" binding="tns:HelloBinding">
      <soap:address location="http://localhost:8080/J2WST/jaxrpc/Hello"/>
    </port>
  </service>
</definitions>
